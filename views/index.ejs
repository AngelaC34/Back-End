    <!-- Testimonies Start -->
    <div class="container-fluid bg-light bg-icon py-6">
        <div class="container">
            <div class="section-header text-center mx-auto mb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 500px;">
                <h1 class="display-5 mb-3">Customer Review</h1>
                <p>Check out the testimonies left by our visitors behind!</p>
            </div>
            <% if (testimonies.length > 0) { %>
                <div class="owl-carousel testimonial-carousel wow fadeInUp" data-wow-delay="0.1s">
                    <% testimonies.forEach(testimony => { %>
                        <div class="testimonial-item position-relative bg-white p-5 mt-4">
                            <i class="fa fa-quote-left fa-3x text-primary position-absolute top-0 start-0 mt-n4 ms-5"></i>
                            <p class="mb-4"><%= testimony.review %></p>
                            <div class="d-flex align-items-center">
                                <div class="mb-4">
                                    <h5 class="mb-1"><%= testimony.name %></h5>
                                </div>
                            </div>
                        </div>
                    <% }); %>
                </div>
            <% } else { %>
                <div class="alert alert-info" role="alert">
                    No testimonies available at the moment.
                </div>
            <% } %>
        </div>
    </div>
    <!-- Testimonies End -->


    <div class="container py-5">
        <div class="row justify-content-center">
            <div class="col-lg-6">
                <div class="card shadow">
                    <div class="card-body">
                        <h2 class="card-title text-center mb-4" style="color: #218838;">Add Testimony</h2>
                        <% if (typeof messages !== 'undefined') { %>
                            <% if (messages.error) { %>
                                <div class="alert alert-danger" role="alert">
                                    <%= messages.error %>
                                </div>
                            <% } %>
                            <% if (messages.success) { %>
                                <div class="alert alert-success" role="alert">
                                    <%= messages.success %>
                                </div>
                            <% } %>
                        <% } %>
                        <form action="/testiRoutes/add-testimony" method="POST">
                            <div class="mb-3">
                                <label for="review" class="form-label">Review:</label>
                                <textarea class="form-control" id="review" name="review" rows="4" placeholder="Write your testimony here..." required></textarea>
                            </div>
                            <div class="d-grid gap-2">
                                <button type="submit" class="btn btn-primary btn-lg">Submit</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
